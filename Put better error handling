import logging
import sys

class QueueSystem:
    def __init__(self):
        self.queue = []

    def enqueue(self, patient):
        try:
            if not isinstance(patient, str):
                raise TypeError("Patient name must be a string")
            self.queue.append(patient)
            print(f"{patient} has been added to the queue.")
        except TypeError as e:
            self.log_error(e)

    def dequeue(self):
        try:
            if len(self.queue) == 0:
                raise IndexError("Queue is empty")
            patient = self.queue.pop(0)
            print(f"{patient} has been dequeued.")
            return patient
        except IndexError as e:
            self.log_error(e)

    def log_error(self, error):
        logging.basicConfig(filename='error.log', level=logging.ERROR)
        logging.error(error)
        print("An error occurred. Please contact support for assistance.")

if __name__ == "__main__":
    queue_system = QueueSystem()

    # Example usage
    queue_system.enqueue("Philip")
    queue_system.enqueue(123)  # Invalid input, should raise TypeError
    queue_system.dequeue()
    queue_system.dequeue()  # Attempt to dequeue from an empty queue, should raise IndexError
